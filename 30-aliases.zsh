alias rm="rm -v"
alias cp="cp -v"
alias mv="mv -v"

alias grep='grep --color=auto'
alias fgrep='fgrep --color=auto'
alias egrep='egrep --color=auto'

alias ls='ls --color'
alias l='ls -l'
alias la='ls -a'
alias lla='ls -la'

alias mtr='mtr --curses'

alias delim="seq  -f "=" -s '' `tput cols`"

alias sysu="systemctl --user"